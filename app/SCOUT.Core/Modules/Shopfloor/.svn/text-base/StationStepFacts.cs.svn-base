namespace SCOUT.Core.Data
{
    public class StationStepFacts
    {
        private Part m_part;
        private string m_result;

        public StationStepFacts(Part part, string result)
        {
            m_part = part;
            m_result = result;
        }

        public Part Part
        {
            get { return m_part; }
        }

        public string Result
        {
            get { return m_result; }
        }
    }
}